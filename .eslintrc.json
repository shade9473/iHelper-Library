{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "vitest": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-essential"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "vue"
  ],
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn"
  },
  "settings": {
    "vue": {
      "version": "detect"
    }
  }
}
